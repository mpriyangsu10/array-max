#include<iostream>

using namespace std;

int largest(){
        
        int array[]={56,42,78,45,59,98};
        
        int max=0;
        
        for(int i=0;i<6;i++){
            if(array[i]>max){
                max=array[i];
            }
        }
        
        return max;
}

int main(){
    
    cout<<largest();
    
    return 0;
}
